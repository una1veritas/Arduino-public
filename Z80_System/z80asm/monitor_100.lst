02D8: C9       [43]     		ret
02D9:                   hex2nib.alpha:
02D9: FE61     [ 7]     		cp 		'a'
02DB: 3806     [14|19]  		jr 		c, hex2nib.upper
02DD: FE67     [21]     		cp 		'f'+1
02DF: 3F       [25]     		ccf		; set carry if A >= 'f'+1
02E0: D8       [30|36]  		ret 	c
02E1: E6DF     [37]     		and 	$df		; a - f to upper char
02E3:                   hex2nib.upper:
02E3: FE41     [ 7]     		cp 		'A'
02E5: D8       [12|18]  		ret 	c
02E6: FE47     [19]     		cp      'F' + 1
02E8: 3F       [23]     		ccf
02E9: D8       [28|34]  		ret 	c      ; error if it is larger than 'F'
02EA: D637     [35]     		sub 	'A'-10
02EC: C9       [45]     		ret
                        
                        ; read hexadecimal string char upto 2 or 4 (set in C) 
                        ; bytes from (HL) and return int val in DE
                        ; if non hexdec char is encountered at (HL), returns with current de value without inc hl.
                        ; if C upper-limit bytes has been read, returns with current de value with increment hl.
                        ; A reg. hold the last char read from (HL).
                        ;
02ED:                   hexstr_de:
02ED: 110000   [10]     		ld      de, 0000h
02F0:                   hexstr_de.loop:
02F0: 7E       [ 7]     		ld      a, (hl)
