02F5: 0604     [ 7]     		ld 		b, 4
02F7:                   hexstr_de.rl4:
02F7: CB13     [ 8]     		rl      e		 ;rotate left entire de
02F9: CB12     [16]     		rl      d
02FB: 10FA     [24|29]  		djnz    hexstr_de.rl4
02FD: 83       [28]     		add 	e
02FE: 5F       [32]     		ld 		e, a
02FF: 23       [38]     		inc 	hl 		; 
0300: 0D       [42]     		dec 	c
0301: 20ED     [49|54]  		jr      nz, hexstr_de.loop
