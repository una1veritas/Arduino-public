                        ; convert one char expressing a hexadecimal digit 
                        ; in A reg. to nibble in A
                        ; set carry flag if got a wrong char
                        ;
02CF:                   hex2nib:
02CF: FE30     [ 7]     		cp 		'0'
02D1: D8       [12|18]  		ret 	c		; A < '0'
02D2: FE3A     [19]     		cp 		'9' + 1
02D4: 3003     [26|31]  		jr 		nc, hex2nib.alpha
